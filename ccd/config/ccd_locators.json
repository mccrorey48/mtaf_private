                },
                "popover_present": {
                    "script": "return window.document.getElementsByClassName('popover-content').length == 1"
                },
                "row_count_ok": {
                    "script": "return ((${desired_count} == 0) && (window.document.getElementsByTagName('tbody').length == 0)) || ((window.document.getElementsByTagName('tbody').length > 0) && (window.document.getElementsByTagName('tbody')[0].getElementsByTagName('tr').length == ${desired_count}))"
                }
            },
            "timeframes": {
