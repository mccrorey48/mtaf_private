                }
            },
            "type": "javascript",
            "user_phones": {
                "add_button": {
                    "script": "return window.document.getElementById('content').getElementsByTagName('div')[3].getElementsByClassName('btn')[0].textContent",
                    "value": "Add Phone"
                }
            },
            "users": {
                "ext_in_table": {
